    "# How to change bulk files(text,image) name sequentially"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "path = \"/home2/aparaye/Car_Color_Model/dataset2/\"#\"path of folder\"\n",
    "files = os.listdir(path)\n",
    "i = 1\n",
    "for file in files:\n",
    "    os.rename(os.path.join(path, file), os.path.join(path, 'New_Name'+str(i)+'.jpg'))\n",
    "    i = i+1"
   ]
  }
